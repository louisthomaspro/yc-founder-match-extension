{
    "manifest_version": 3,
    "name": "YC Founder Match",
    "version": "REPLACED-BY-VITE",
    "description": "Analyze YC Startup School co-founder candidates against your ideal profile criteria",
    "icons": {
        "34": "assets/icons/icon-34.png",
        "128": "assets/icons/icon-128.png"
    },
    "action": {
        "default_title": "YC Founder Match Settings",
        "default_popup": "src/popup/index.html",
        "default_icon": "assets/icons/icon-34.png"
    },
    "content_scripts": [
        {
            "js": ["src/content/index.ts"],
            "matches": ["https://www.startupschool.org/cofounder-matching/candidate/*"]
        }
    ],
    "background": {
        "service_worker": "src/background/index.ts"
    },
    "web_accessible_resources": [
        {
            "resources": ["assets/icons/*.png"],
            "matches": ["https://www.startupschool.org/*"],
            "extension_ids": []
        }
    ],
    "permissions": ["storage", "tabs", "activeTab"],
    "host_permissions": ["https://api.openai.com/*"]
}
